<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <RootNamespace></RootNamespace>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.0.0</Version>
    <Authors>Anu6is</Authors>
	<Title>Believe.Net</Title>
	<Product>.Net UnbelievaBoat API (unb-api-net)</Product>
	<PackageIconUrl>https://raw.githubusercontent.com/Anu6is/unb-api-net/master/Believe.Net/assets/logo_red.png?token=ABDCC3ISIBB6OYMBF44LXXC45K5UM</PackageIconUrl>
    <Description>A .Net wrapper for the UnbelievaBoat Discord bot API.</Description>
    <PackageProjectUrl>https://github.com/Anu6is/unb-api-net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Anu6is/unb-api-net</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>Discord, UnbelievaBoat, .Net-Core, unb-api, Believe.Net</PackageTags>
	<PackageId>Believe.Net</PackageId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.2" />
  </ItemGroup>

</Project>
